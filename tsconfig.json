{
  "compilerOptions": {
    "declaration": true,
    "experimentalDecorators": true,
    "inlineSources": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "stripInternal": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5"
  }
}
